{
   "version": "0.2.0",
   "configurations": [
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build web api",
            "program": "${workspaceRoot}/cappanna-helper-api/bin/Debug/netcoreapp2.0/CappannaHelper.Api.dll",
            "args": [],
            "cwd": "${workspaceRoot}/cappanna-helper-api",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
}